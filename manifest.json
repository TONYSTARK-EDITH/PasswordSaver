{
  "name": "Password Generator",
  "description": "It generates a strong password",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "clipboardWrite",
    "clipboardRead"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Password Generator",
    "default_icon": {
      "16": "padlock.png",
      "48": "padlock.png",
      "128": "padlock.png"
    }
  }
}